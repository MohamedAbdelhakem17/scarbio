{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "import/no-unresolved": "error"
  },
  "settings": {
    "react": { "version": "detect" }
  }
}